{
  "name": "webpack-tailwind-boilerplate",
  "version": "1.0.0",
  "description": "A Minimal Webpack 5/4 boilerplate with Tailwind CSS 3, TypeScript, PostCSS, Prettier And More.",
  "main": "index.js",
  "scripts": {
    "start-dev": "webpack serve --progress --env dev --config webpack.dev.js",
    "build": "webpack --progress --env prod --config webpack.prod.js"
